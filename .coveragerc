[run]
branch = True
source = .

omit =
    */migrations/*
    */settings/*
    */config/*
    */sphinx/*
    */tests/*
    *urls*
    *apps.py*
    *__init__.py
    manage.py

[report]
exclude_lines =
    pragma: no cover
    raise NotImplementedError

ignore_errors = True

[html]
directory = coverage/html
